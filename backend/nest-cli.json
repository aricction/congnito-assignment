{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "root": "./",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "prisma/generated/prisma/**/*",
        "outDir": "dist/prisma",
        "watchAssets": true
      }
    ]
  }
}
